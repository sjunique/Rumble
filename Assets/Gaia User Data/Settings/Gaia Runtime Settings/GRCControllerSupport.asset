  m_customCharacterLabel:
    m_Text: Custom Character
    m_Image: {fileID: 0}
    m_Tooltip: Put your character from the scene into this slot. If there is no dedicated
      character, you can usually assign the camera object in here. All the necessary
      settings and references will then be automatic applied when you create runtime.
    m_TextWithWhitespace: "Custom Character\u200B"
  m_customCameraLabel:
    m_Text: Custom Camera
    m_Image: {fileID: 0}
    m_Tooltip: Put your main camera from the scene into this slot. All the necessary
      settings and references will then be automatic applied when you create runtime.
    m_TextWithWhitespace: "Custom Camera\u200B"
